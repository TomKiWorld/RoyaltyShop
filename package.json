{
  "name": "royalty-shop",
  "version": "0.1.0",
  "author": "TomKiWorld",
  "description": "E-commerse shop",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/TomKiWorld/RoyaltyShop"
  },
  "homepage": "https://tomkiworld.github.io/RoyaltyShop/",
  "engines": {
    "node": "13.14.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "client": "cp ./.env ./client/.env && cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cp ./.env ./client/.env && cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "predeploy": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "deploy": "gh-pages -d client/build",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "stripe": "^8.68.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.19.1"
  }
}
